
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Error</title>
</head>
<body>
    <h2>HTTP @ViewBag.StatusCode Error</h2>
    <p>
        <strong class="alert-warning">@ViewBag.Message</strong>
    </p>
    <hr />
    <h5>Debug Information:</h5>
    <p>
        @ViewBag.StackTrace
    </p>


</body>
</html>
